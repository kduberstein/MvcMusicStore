<?xml version="1.0" encoding="utf-8"?>

<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="MusicStore.Model" namespace="MusicStore.Model.Products">

  <class name="Artist" table="`ARTIST`">

    <id name="Id" access="property" column="`ARTIST_ID`">
      <generator class="guid.comb" />
    </id>

    <version name="Version" column="`VERSION`" />

    <property name="Name" type="String" column="`NAME`" not-null="true" unique="true" />

  </class>

</hibernate-mapping>